  "source": "// SPDX-License-Identifier: MIT\npragma solidity ^0.7.4;\npragma experimental ABIEncoderV2;\n\ncontract OnceFunctionCaller {\n    mapping (bytes32 => bool) private _isCalled;\n\n    modifier onlyOnce(string memory _functionName) {\n        bytes32 bytesFunctionName = keccak256(abi.encodePacked(_functionName));\n\n        require (_isCalled[bytesFunctionName] == false, \"This function has been already called\");\n\n        _;\n\n        _isCalled[bytesFunctionName] = true;\n    }\n}",
